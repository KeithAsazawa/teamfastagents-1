<div class="row-fluid">
  <div class="span9 text-center">
    <img src="images/fastagentsblog.png" class="blog_image">
  </div>
</div>
<!-- <div class="row-fluid"></div> -->
  <div class="span9 blog_body text-center">  
    <% @blogs.each do |blog| %>
      <div class="span4 blogs text-center">
      <ul>
        <li class="blog_title"><h4><%= link_to blog.title, blog_path(blog) %></h4></li>
        <li><%= image_tag blog.image.url(:medium) %></li><br />
        <li><%= truncate(blog.content, :length => 400) %></li>
        <!-- <li><%= link_to 'Show', blog %></li> -->
        <li class="time">Posted <%= time_ago_in_words(blog.created_at) %> ago.</li>
        <li><%= link_to 'Read more...', blog_path(blog) %> </li>
        <!-- <li><%= link_to 'Edit', edit_blog_path(blog) %></li>
        <li><%= link_to 'Delete', blog, method: :delete, data: { confirm: 'Are you sure?' } %></li> -->
      </ul>
      </div>
    <% end %>
  </div>
